#include<stdio.h>
int main()
{
int a[100],i,n,j,val;
scanf("%d",&n);
    for (i=0;i<n;i++)
    {
     scanf("%d",&a[i]);
    }
    for (i=0;i<n;i++)
    {
        printf("%d\n",a[i]);
    }
    n+=1;
    printf("Enter the position ");
    scanf("%d",&j);
    for(i=n;i>=j;i--)
    {
        a[i]=a[i-1];
    }
    printf("Enter the element to be inserted");
    scanf("%d",&val);
    a[j-1]=val;
     for (i=0;i<n;i++)
    {
        printf("%d\n",a[i]);
    }
return 0;
}     
